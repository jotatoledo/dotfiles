[core]
	autocrlf = true
[alias]
	undo = reset HEAD~ --soft
	f-me = reset --hard HEAD
	f-you = push --force
	amend = commit --amend
	prune = git fetch --prune
[rebase]
	autosquash = true
[include]
  path = ./.gitconfig.user
[filter "lfs"]
	clean = git-lfs clean -- %f
	smudge = git-lfs smudge -- %f
	process = git-lfs filter-process
	required = true
